<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="testProps" class="org.springframework.beans.factory.config.PropertiesFactoryBean">
        <property name="location" value="automation-credentials.properties"/>
    </bean>

    <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="location" value="automation-credentials.properties"/>
    </bean>

    <util:map id="createLabel" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="repository" value="${github.repository}" />
        <entry key="labelName" value="some-label" />
        <entry key="color" value="f29513" />
    </util:map>

    <util:map id="watchers" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="owner" value="${github.owner}" />
        <entry key="repositoryName" value="${github.repository}" />
    </util:map>

    <util:map id="collaborators" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="user" value="vgulyakin" />
        <entry key="repositoryName" value="${github.repository}" />
    </util:map>

    <util:map id="deploykey" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="repositoryName" value="${github.repository}" />
        <entry key="title" value="test title" />
        <entry key="key" value="ssh-rsa AAAAB3NzaC1yc2EAAAABJQAAAIBjxppb4IbD7j6yZnP/x8E5QREDIsyOHRoMmMoW+f34gExpRpYj03d4y0zshDRHcq875zfPrCswJH/ANQRatUO8nyHypjltiHKMqPGI0wwuYOmJReM1ioDzD+q27N2nrrfEcshxkCp9v7iPmw1p2PvKgvcEmHmCqKYMh299us0Hgw==" />
    </util:map>

    <util:map id="commits" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="repositoryName" value="${github.repository}" />
        <entry key="body" value="commit comment body" />
    </util:map>

    <util:map id="downloads" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.owner}" />
        <entry key="user" value="${github.userName}" />
        <entry key="repositoryName" value="${github.repository}" />
    </util:map>

    <util:map id="user" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
    </util:map>
    
    <util:map id="updateCurrentUser" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="blog" value="blogs.mulesoft.org" />
        <entry key="location" value="Buenos Aires" />
        <entry key="company" value="MuleSoft" /> 
    </util:map>
    
    <util:map id="follow" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="follow" value="vgulyakin" />
    </util:map>

    <util:map id="email" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="email" value="email1@mulesoft.com" />
    </util:map>

    <util:map id="key" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
    	<entry key="title" value="title" />
    	<entry key="key" value="ssh-rsa AAAAB3NzaC1yc2EAAAABJQAAAIBjxppb4IbD7j6yZnP/x8E5QREDIsyOHRoMmMoW+f34gExpRpYj03d4y0zshDRHcq875zfPrCswJH/ANQRatUO8nyHypjltiHKMqPGI0wwuYOmJReM1ioDzD+q27N2nrrfEcshxkCp9v7iPmw1p2PvKgvcEmHmCqKYMh299us0Hgw== rsa-key-20130923" />
    </util:map>

    <util:map id="gist" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="description" value="gist description" />
        <entry key="filename" value="file.txt" />
        <entry key="content" value="gist content" />
        <entry key="body" value="comment body" />
        <entry key="updatedBody" value="updated comment body" />
    </util:map>

    <util:map id="milestone" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="repository" value="${github.repository}" />
        <entry key="state" value="open" />
        <entry key="title" value="1.0" />
        <entry key="description" value="Milestone 1.0 description" />
    </util:map>

    <util:map id="issue" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="owner" value="${github.userName}" />
        <entry key="repository" value="${github.repository}" />
        <entry key="title" value="issue title" />
        <entry key="body" value="issue body" />
        <entry key="state" value="open" />
        <entry key="comment" value="issue comment" />
        <entry key="updatedComment" value="updated issue comment" />
    </util:map>

    <util:map id="reference" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="repository" value="${github.repository}" />
        <entry key="referenceName" value="refs/heads/test-branch" />
        <entry key="branch" value="test-branch" />
    </util:map>

    <util:map id="pullRequest" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="repository" value="${github.repository}" />
        <entry key="title" value="Pull Request Title" />
        <entry key="updatedTitle" value="Updated Pull Request Title" />
        <entry key="body" value="Pull Request Body" />
        <entry key="comment" value="A Pull Request Comment" />
        <entry key="updatedComment" value="An Updated Pull Request Comment" />
        <entry key="reply" value="Test Reply" />
    </util:map>


</beans>