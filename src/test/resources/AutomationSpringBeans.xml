<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="testProps" class="org.springframework.beans.factory.config.PropertiesFactoryBean">
        <property name="location" value="automation-credentials.properties"/>
    </bean>

    <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="location" value="automation-credentials.properties"/>
    </bean>

    <util:map id="createLabel" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="repository" value="${github.repository}" />
        <entry key="labelName" value="some-label" />
        <entry key="color" value="f29513" />
    </util:map>

    <util:map id="watchers" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="owner" value="${github.owner}" />
        <entry key="repositoryName" value="${github.repository}" />
    </util:map>

    <util:map id="collaborators" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="user" value="vgulyakin" />
        <entry key="repositoryName" value="${github.repository}" />
    </util:map>

    <util:map id="deploykey" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="repositoryName" value="${github.repository}" />
        <entry key="title" value="test title" />
        <entry key="key" value="ssh-rsa AAAAB3NzaC1yc2EAAAABJQAAAIBjxppb4IbD7j6yZnP/x8E5QREDIsyOHRoMmMoW+f34gExpRpYj03d4y0zshDRHcq875zfPrCswJH/ANQRatUO8nyHypjltiHKMqPGI0wwuYOmJReM1ioDzD+q27N2nrrfEcshxkCp9v7iPmw1p2PvKgvcEmHmCqKYMh299us0Hgw==" />
    </util:map>

    <util:map id="commits" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="repositoryName" value="${github.repository}" />
        <entry key="body" value="commit comment body" />
    </util:map>

    <util:map id="downloads" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.owner}" />
        <entry key="user" value="${github.userName}" />
        <entry key="repositoryName" value="${github.repository}" />
    </util:map>

    <util:map id="user" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
    </util:map>

    <util:map id="gist" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="description" value="gist description" />
        <entry key="filename" value="file.txt" />
        <entry key="content" value="gist content" />
        <entry key="body" value="comment body" />
        <entry key="updatedBody" value="updated comment body" />
    </util:map>

    <util:map id="milestone" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="repository" value="${github.repository}" />
        <entry key="state" value="open" />
        <entry key="title" value="1.0" />
        <entry key="description" value="Milestone 1.0 description" />
    </util:map>

</beans>