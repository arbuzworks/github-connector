<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="testProps" class="org.springframework.beans.factory.config.PropertiesFactoryBean">
        <property name="location" value="${GITHUB_CREDENTIALS}"/>
    </bean>

    <bean class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
        <property name="location" value="${GITHUB_CREDENTIALS}"/>
    </bean>

    <util:map id="createLabelTestData" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="repository" value="${github.repository}" />
        <entry key="labelName" value="some-label" />
        <entry key="color" value="f29513" />
    </util:map>

    <util:map id="watchersTestData" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="user" value="${github.userName}" />
        <entry key="owner" value="${github.owner}" />
        <entry key="repositoryName" value="${github.repository}" />
    </util:map>

    <util:map id="collaboratorsTestData" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
        <entry key="owner" value="${github.userName}" />
        <entry key="user" value="${github.collaborator}" />
        <entry key="repositoryName" value="${github.repository}" />
    </util:map>

    <util:map id="deployKeyTestData" map-class="java.util.HashMap" key-type="java.lang.String" value-type="java.lang.Object" scope="prototype">
